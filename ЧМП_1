import math
def f (x1, x1_1, x2, x2_2):
    dx1 = abs((math.sqrt(x1) - x1_1)/ math.sqrt (x1))
    dx2 = abs((x2 - x2_2)/x2)
    if (dx1>dx2):
        print ("Друга рівність точніше")
    else: 
        print("Перша рівність точніше")

x1=4.8 #точне число sqrt(x1 )
x2=6/7 #точне число x2
x1_1 = 2.19 #наближене число x1
x2_2 = 0.857 #наближене число x2

f(x1,x1_1,x2,x2_2)
